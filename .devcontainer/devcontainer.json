{
	"name": "A simple api server.",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "api-server",
  "workspaceFolder": "/app",
	"settings": { 
		"terminal.integrated.defaultProfile.windows": "Bash",
		"terminal.integrated.profiles.linux": {
			"Bash": {
				"path": 	"/bin/bash"
			}
		},
		"go.toolsManagement.checkForUpdates": "local",
		"go.gopath": "/go",
		"go.useLanguageServer": true
	},
	"extensions": [
		"golang.Go"
	],
	"remoteUser": "vscode"
}
